  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint",
    "globals": [
      "$Diff",
      "$Keys",
      "$Values",
      "Blob",
      "ClipboardEvent",
      "File",
      "IntervalID",
      "HTMLCanvasElement",
      "HTMLElement",
      "HTMLInputElement",
      "KeyboardEvent",
      "MouseEvent",
      "ReactComponent",
      "ReactElement",
      "Response",
      "SyntheticEvent",
      "SyntheticClipboardEvent",
      "SyntheticCompositionEvent",
      "SyntheticInputEvent",
      "SyntheticUIEvent",
      "SyntheticFocusEvent",
      "SyntheticKeyboardEvent",
      "SyntheticMouseEvent",
      "SyntheticDragEvent",
      "SyntheticWheelEvent",
      "SyntheticTouchEvent"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
